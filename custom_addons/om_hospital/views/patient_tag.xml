<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>
        <record id="view_action_patient_tag_tree" model="ir.ui.view">
            <field name="name">action.patient.tag.tree</field>
            <field name="model">patient.tag</field>
            <field name="arch" type="xml">
                <tree string="Patient Tags" editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>
         <record id="view_action_patient_tag_form" model="ir.ui.view">
            <field name="name">action.patient.tag.form</field>
            <field name="model">patient.tag</field>
            <field name="arch" type="xml">
                <tree string="Patient Tags" editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="action_patient_tag" model="ir.actions.act_window">
            <field name="name">Patient Tags</field>
            <field name="res_model">patient.tag</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a patient tags
                </p>
            </field>
        </record>
    </data>
</odoo>